class MinStack:
    
    def __init__(self):
        self.a=[]
        Self.top=0

    def push(self, val):
        a[top]=val
        top+=1

    def pop(self):
        x=a[top]
        top-=1
        return(x)
    
    def top(self):
        return a[top]
        

    def getMin(self):
        min(self.a)
        
minStack = MinStack();
minStack.push(-2)
minStack.push(0) 
minStack.push(-3)
minStack.getMin()
minStack.pop()
minStack.top()
minStack.getMin()
