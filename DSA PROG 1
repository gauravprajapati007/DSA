#include <iostream>
using namespace std;

int stack[], minimum [] ,n=10, top=-1,min=-1;

void push(val) {
   While(top<n-1){
      stack[++top]=val;
      If(stack[top-1]>val){
         Minimum[++min]=val
  }
}
void pop() {
   if(top<=-1)
   break;
   else{
      stack[top--];
   }
}

void getMin() {
   Cout<<minimum[min];
}

Void top() {
   Cout<<top;

int main() {
push(-2);
push(0);
push(-3);
getMin();
pop();
top();  
getMin();
