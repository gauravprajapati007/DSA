#include <iostream>
using namespace std;

int stack[], minimum [] ,n=10, top=-1;
void push(val) {
   if(top>=n-1)
   cout<<"Stack Overflow"<<endl;
   else {
      stack[++top]=val;
   }
}
void pop() {
   if(top<=-1)
   cout<<"Stack Underflow"<<endl;
   else {
      cout<<"The popped element is "<< stack[top--] <<endl;
   }
}

void display() {
   if(top>=0) {
      cout<<"Stack elements are:";
      for(int i=top; i>=0; i--)
      cout<<stack[i]<<" "
      Count<<endl;
   } else
   cout<<"Stack is empty";
}
